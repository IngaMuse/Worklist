<mvc:View
	controllerName="zjblessons.Worklist.controller.Object"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

<Page
		showFooter="false"
		title="{DocumentNumber}"
>
		<content>
			<IconTabBar expandable="false" headerMode="Inline" selectedKey="{= ${objectView>/sSelectedTab}}">
					<items>
					<IconTabFilter icon="{i18n>iListIcon}" key="List" text="{i18n>List}">
		
					</IconTabFilter>
					<IconTabFilter icon="{i18n>iFormIcon}" key="Form" text="{i18n>Form}">
		
					</IconTabFilter>
					</items>
			</IconTabBar>
		</content>
</Page>
</mvc:View>